<h1>Listing Saved Stocks</h1>
<%= link_to 'New Stock', new_stock_path %>
<table>
  <tr>
    <th>Title</th>
  </tr>
 
  <% @stocks.each do |stock| %>
    <tr>
      <td><%= stock.title %></td>
      <td><%= link_to 'Show', stock_path(stock) %></td>
      <td><%= link_to 'Edit', edit_stock_path(stock) %></td>
      <td><%= link_to 'Destroy',
                                stock_path(stock),
                                method: :delete,
                                data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>